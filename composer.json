{
    "name": "marcusrox/asaas-php-sdk",
    "type": "library",
    "description": "Asaas.com PHP API v3 Wrapper",
    "keywords": [
        "Asaas",
        "API",
        "Payment SaaS",
        "Credit Card",
        "Boleto",
        "PIX"
    ],
    "homepage": "https://github.com/marcusrox/asaas-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Adriano Carrijo",
            "email": "adrianovieirac@gmail.com",
            "homepage": "https://moblu.es"
        },
        {
            "name": "AgÃªncia Softr",
            "email": "agencia.softr@gmail.com",
            "homepage": "http://www.softr.com.br"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-faker": "2.x-dev"
    },
    "suggest": {
        "guzzlehttp/guzzle": "To use GuzzleHttpAdapter, require guzzlehttp/guzzle:^7.0."
    },
    "autoload": {
        "psr-4": {
            "Adrianovcar\\Asaas\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
